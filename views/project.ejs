<!DOCTYPE html>
<html>
  <head>
    <% include partials/head %>
  </head>
  <body>
    <% include partials/header %>

    <% if( page_url == "/bitelabs" ){ %>
      <% include partials/bitelabs_page %>
    <% } %>

    <% if( page_url == "/genecoin" ){ %>
      <% include partials/genecoin_page %>
    <% } %>

    <% if( page_url == "/pinkhouse" ){ %>
      <% include partials/pinkhouse_page %>
    <% } %>
    
    <% if( page_url == "/mcmass" ){ %>
      <% include partials/mcmass_page %>
    <% } %>
    
    <% if( page_url == "/yinte" ){ %>
      <% include partials/yinte_page %>
    <% } %>

    <% if( page_url == "/beastmade" ){ %>
      <% include partials/beastmade_page %>
    <% } %>


    <div class="share_project">
      <div class="share_project_label">Share this project</div>

      <div class="share_project_social_container">
        <a href="https://www.facebook.com/sharer/sharer.php?u=http://www.hellovelocity.com<%= page_url %>" target="_blank">
          <div class="share_project_social">
            <div class="share_project_social_icon">
              <i class="fa fa-facebook fa"></i>
            </div>
            <div class="share_project_social_label">
              facebook
            </div>
          </div>
        </a>
        <a href="https://twitter.com/home?status=hellovelocity.com<%= page_url %>%20%40hellovelocity" target="_blank">
          <div class="share_project_social">
            <div class="share_project_social_icon">
              <i class="fa fa-twitter fa"></i>
            </div>
            <div class="share_project_social_label">
              twitter 
            </div>
          </div>
        </a>
        <a href="https://plus.google.com/share?url=http://www.hellovelocity.com<%= page_url %>" target="_blank">
          <div class="share_project_social">
            <div class="share_project_social_icon">
              <i class="fa fa-google-plus fa"></i>
            </div>
            <div class="share_project_social_label">
              google+
            </div>
          </div>
        </a>
        <a href="https://www.linkedin.com/shareArticle?mini=true&url=http://www.hellovelocity.com<%= page_url %>&title=Hello%20Velocity&summary=&source=" target="_blank">
          <div class="share_project_social">
            <div class="share_project_social_icon">
              <i class="fa fa-linkedin fa"></i>
            </div>
            <div class="share_project_social_label">
              linkedin
            </div>
          </div>
        </a>
        <a href="mailto:?subject=Hello%20Velocity%20&body=http://www.hellovelocity.com<%= page_url %>" target="_blank">
          <div class="share_project_social">
            <div class="share_project_social_icon">
              <i class="fa fa-envelope fa"></i>
            </div>
            <div class="share_project_social_label">
              email
            </div>
          </div>
        </a>
      </div>

      <div class="share_project_link">
        hellovelocity.com<%= page_url %>
      </div>
    </div>

    <div class="other_projects_container">
      <div class="other_projects">
        <div class="other_projects_label">More Projects</div>
        <div class="other_projects_array">
          <% if( page_url != "/yinte" ) { %>
            <% include partials/yinte_tile %>
          <% } %>
          <% if( page_url != "/mcmass" ) { %>
            <% include partials/mcmass_tile %>
          <% } %>
          <% if( page_url != "/pinkhouse" ) { %>
            <% include partials/pinkhouse_tile %>
          <% } %>
          <% if( page_url != "/bitelabs" ) { %>
            <% include partials/bitelabs_tile %>
          <% } %>
          <% if( page_url != "/beastmade" ) { %>
            <% include partials/beastmade_tile %>
          <% } %>
          <% if( page_url != "/genecoin" ) { %>
            <% include partials/genecoin_tile %>
          <% } %>
        </div>
      </div>

      <% include partials/footer%>
    </div>

    <script src="/js/jquery-2.1.3.min.js"></script>
    <script src="/js/index.js"></script>
  </body>
</html>
